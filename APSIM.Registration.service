[Unit]
Description=APSIM.Registration Web Services

[Service]
WorkingDirectory=/var/www/APSIM.Registration
#ExecStart=/usr/bin/dotnet /var/www/APSIM.Registration/APSIM.Registration.Service.dll
ExecStart=/var/www/APSIM.Registration/APSIM.Registration.Service
Restart=always
# Restart service after 10 seconds if the dotnet service crashes:
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=APSIM.Registration
Environment=ASPNETCORE_ENVIRONMENT=Production 

[Install]
WantedBy=multi-user.target
